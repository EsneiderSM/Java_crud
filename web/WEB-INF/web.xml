<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>ControllerUsuario</servlet-name>
        <servlet-class>Controller.ControllerUsuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControllerLogin</servlet-name>
        <servlet-class>Controller.ControllerLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControllerTipoUsuario</servlet-name>
        <servlet-class>Controller.ControllerTipoUsuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControllerFranquicia</servlet-name>
        <servlet-class>Controller.ControllerFranquicia</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControllerPago</servlet-name>
        <servlet-class>Controller.ControllerPago</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControllerCompra</servlet-name>
        <servlet-class>Controller.ControllerCompra</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControllerTarjetaXCliente</servlet-name>
        <servlet-class>Controller.ControllerTarjetaXCliente</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControllerTipoTarjeta</servlet-name>
        <servlet-class>Controller.ControllerTipoTarjeta</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ControllerUsuario</servlet-name>
        <url-pattern>/Usuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControllerLogin</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControllerTipoUsuario</servlet-name>
        <url-pattern>/TipoUsuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControllerFranquicia</servlet-name>
        <url-pattern>/Franquicia</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControllerPago</servlet-name>
        <url-pattern>/Pago</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControllerCompra</servlet-name>
        <url-pattern>/Compra</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControllerTarjetaXCliente</servlet-name>
        <url-pattern>/TarjetaXCliente</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControllerTipoTarjeta</servlet-name>
        <url-pattern>/TipoTarjeta</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
